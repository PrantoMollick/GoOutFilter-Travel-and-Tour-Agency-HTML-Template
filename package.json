{
  "name": "gooutfilter-travel-tour-agency-html-template",
  "version": "1.0.0",
  "description": "Travel and Tour Agency HTML Template",
  "browserslist": [
    "last 2 versions",
    "> 1%"
  ],
  "scripts": {
    "dev": "live-server",
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "start": "npm-run-all --parallel watch:sass dev",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "parcel:dev": "parcel index.html --dist-dir ./dist",
    "build": "parcel build index.html --dist-dir ./dist",
    "parcel:watch": "parcel watch index.html"
  },
  "keywords": [
    "HTML",
    "CSS",
    "SCSS",
    "SASS",
    "JS"
  ],
  "author": "Pranto Mollick",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.8.3",
    "autoprefixer": "^10.4.14",
    "concat": "^1.0.3",
    "css-loader": "^6.7.3",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.8.3",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "postcss-cli": "^10.1.0"
  },
  "dependencies": {
    "daterangepicker": "^3.1.0",
    "jquery": "^3.6.4",
    "moment": "^2.29.4"
  }
}
